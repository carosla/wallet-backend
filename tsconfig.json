{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "strict": true,
      "baseUrl": "./",
      "paths": {
        "@src/*": ["./src/*"]
      },
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true
    },
    
    "include": [
    "src/**/*"
  ],
  "exclude": ["node_modules", "dist"]
}
